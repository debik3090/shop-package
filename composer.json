{
    "name": "debik3090/shop-package",
    "description": "Laravel 12 shop package with products management",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Debik3090",
            "email": "nekittchubov@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Debik3090\\ShopPackage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Debik3090\\ShopPackage\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Debik3090\\ShopPackage\\ShopServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
