{
    "name": "vendor/shop-package123",
    "description": "Laravel 12 shop package with products management",
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your@email.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vendor\\ShopPackage\\ShopServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Vendor\\ShopPackage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vendor\\ShopPackage\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
